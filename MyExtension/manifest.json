{
    "manifest_version": 3,
    "name": "My Extension",
    "description": "An example extension with a switch and 3 buttons",
    "version": "1.0.01",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_icon": "burger_king.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://twitter.com/*"],
        "js": ["content.js"]
    }]
}

